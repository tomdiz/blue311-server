{
  "name": "blue311-Server",
  "description": "A simple map viewing web application and REST client for blue311 iOS clients, powered by MongoDB and Leaflet maps. This supports finding parking spaces, building entrances and general information point on a map. You can also add comments to map points.",
  "version": "0.1.0",
  "private": false,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "dependencies": {
    "config-multipaas": "^0.1.0",
    "mongojs": "^0.17.0",
    "restify": "^2.8.3",
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "ejs": "~0.8.5",
    "express": "~4.9.0",
    "morgan": "~1.3.0",
    "mongoose": "*"
  },
  "devDependencies": {
    "request": "2.24.x",
    "tap": "^0.4.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomdiz/blue311-server.git"
  },
  "scripts": {
    "start": "node server.js",
    "build": "npm run initdb",
    "flushdb": "node bin/flushdb.js",
    "initdb": "node bin/bootstrap.js"
  },
  "main": "server.js"
}
